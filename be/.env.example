# Existing application settings
DATABASE_URL=postgresql://user:password@localhost:5432/dbname
SECRET_KEY=your-secret-key-here
GOOGLE_CLIENT_ID=your-google-client-id
PROJECT_NAME=Shifty
DEBUG=False
MODE=development

# RFID Tracking System Settings
# Optional: Use separate database for RFID system (defaults to DATABASE_URL if not set)
# RFID_DATABASE_URL=postgresql://user:password@localhost:5432/rfid_db

# CF-H906 RFID Reader Configuration
RFID_READER_IP=192.168.1.100
RFID_READER_PORT=4001
RFID_CONNECTION_TYPE=wifi  # Options: wifi, bluetooth, serial
RFID_READER_ID=CF-H906-001

# Logging
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR

# CORS Origins (already configured, but you can add more)
# BACKEND_CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173"]
