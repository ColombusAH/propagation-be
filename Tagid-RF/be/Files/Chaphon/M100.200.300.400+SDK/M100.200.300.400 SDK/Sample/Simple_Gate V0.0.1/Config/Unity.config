<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Unity.Configuration"/>
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
	<unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
		<container>
			<!--DataAccess-->
			<register type="Simple_Gate.IDataAccess.IBaseAccess, Simple_Gate" mapTo="Simple_Gate.DataAccess.BaseAccess, Simple_Gate"/>
			<register type="Simple_Gate.IDataAccess.IDBContext, Simple_Gate" mapTo="Simple_Gate.DataAccess.DBContext, Simple_Gate"/>
			<register type="Simple_Gate.IDataAccess.IInOutBoundAccess, Simple_Gate" mapTo="Simple_Gate.DataAccess.InOutBoundAccess, Simple_Gate"/>
		</container>
	</unity>
</configuration>